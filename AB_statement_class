from dataclasses import dataclass
from typing import List, ClassVar

@dataclass
class Job:
    all_jobs: ClassVar[List['Job']] = []# Jobインスタンスを保持するクラス変数
    job_name:str
    #1
    animate     :str # This value is "y" if this job primarily deals with animate, otherwise it's blank. For example, a doctor or a hunter focuses on living things, but a pizza deliverer or a chef focuses on food, which is non-living things.
    animal      :str # This value is "y" if this job primarily deals with animal, otherwise it's blank.
    child       :str # This value is "y" if this job primarily deals with children, otherwise it's blank.
    #2
    occupation  :str # This value is 'y' if most people consider this to be a job, otherwise it's blank.
    negative    :str # If the worker on this job is busy, which means a negative situation, this value is "y".
    take_money  :str # Does this job take money directly from customers when providing services or products?
    #3
    offer_coupon:str # Does this job provide coupons to customers?
    high_salary :str # Is it common for this job generally pay well?
    teen        :str # Is it common for high school students to engage in this job?
    #4
    w_work      :str # Is it common for people in this job to hold multiple jobs?
    error_die   :str # Does this job involve direct responsibility for people's lives? like a doctor or a bodyguard.
    monitoring  :str # Does this profession monitor lifeforms?
    #5
    burn            :str # Does this job burn something?
    cut             :str # Does this job cut something?
    carry           :str # Does this job carry something?
    #6
    drive           :str # Does this job drive something?
    attack          :str # Does this job involve hitting, hurting, destroying, invading or killing something?
    nurture         :str # Is this job a job that nurtures or grows something?
    #7
    animate_product :str # Does this job treat life forms as products?
    blood           :str # Does this job related to blood?
    food            :str # Does this job related to food?
    #8
    fix             :str # Is this job fix or improve something?
    intelligence    :str # Does this job require a high level of intelligence?
    strength        :str # Does this job require a high level of physical ability or strength?
    #9
    bed             :str # Does this job related to bed?
    sit             :str # Does this job related to sit?
    #10
    industry            :str
    verb                :list
    target              :list
    def __post_init__(self):
        Job.all_jobs.append(self)

