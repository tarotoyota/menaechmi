class AB_statement_mas:
    def __init__(self, s_Job, s_C_statement, s_D_statement):
        self.s_Job, self.s_C_statement, self.s_D_statement = s_Job, s_C_statement, s_D_statement
_empty_                     = AB_statement_mas(job__                            , C__                   , D__)
