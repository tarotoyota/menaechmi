def find_similar_jobs(input_statement: AB_statement_mas) -> List[Job]:
    input_job = input_statement  # .s_Jobを通じてJobインスタンスにアクセス
    similarity_scores = {}

    input_job_attributes = [input_job.animate, input_job.animal, input_job.child,
                            input_job.occupation, input_job.negative, input_job.take_money,
                            input_job.offer_coupon, input_job.high_salary, input_job.teen,
                            input_job.w_work, input_job.error_die, input_job.monitoring,
                            input_job.burn, input_job.cut, input_job.carry,
                            input_job.drive, input_job.attack, input_job.nurture,
                            input_job.animate_product, input_job.blood, input_job.food,
                            input_job.fix, input_job.intelligence, input_job.strength,
                            input_job.bed, input_job.sit]


    for job in Job.all_jobs:
        if job == input_job or job.industry == input_job.industry:
            # 入力されたジョブ自身または同じ.industryのジョブは比較から除外
            continue

        compare_job_attributes = [job.animate, job.animal, job.child,
                                  job.occupation, job.negative, job.take_money,
                                  job.offer_coupon, job.high_salary, job.teen,
                                  job.w_work, job.error_die, job.monitoring,
                                  job.burn, job.cut, job.carry,
                                  job.drive, job.attack, job.nurture,
                                  job.animate_product, job.blood, job.food,
                                  job.fix, job.intelligence, job.strength,
                                  job.bed, job.sit]

        similarity_score = sum(1 for i, j in zip(input_job_attributes, compare_job_attributes) if i == j and i != "")
        similarity_scores[id(job)] = similarity_score

    top_similar_jobs = sorted((job for job in Job.all_jobs if id(job) in similarity_scores), key=lambda job: similarity_scores[id(job)], reverse=True)[:5]
    return top_similar_jobs

def get_similar_job_names(input_statement: AB_statement_mas) -> List[str]:
    similar_jobs = find_similar_jobs(input_statement)
    return [job.job_name for job in similar_jobs]

# 使用例
# similar_job_names = get_similar_job_names(AB_statement_mas(job_pediatrician))
# print("\n".join(similar_job_names))


def get_words_class_AB_statement_mas():
    instances_list = [
        name for name, obj_instance in globals().items()
        if isinstance(obj_instance, AB_statement_mas)
    ]
    return instances_list
